{
	"name": "timewalk-vscode",
	"displayName": "TimeWalk-VSCode",
	"description": "Report your coding time and metrics",
	"version": "0.0.2",
	"license": "MIT",
	"publisher": "desmondlzy",
	"author": {
		"name": "Desmond Zhenyuan Liu"
	},
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/desmondlzy/timewalk-vscode"
	},
	"extensionKind": [
		"workspace"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "timewalk.debug",
				"title": "TimeWalk: Debug"
			},
			{
				"command": "timewalk.statusBarEnabled",
				"title": "TimeWalk: Enable/Disble Status Bar Information"
			},
			{
				"command": "timewalk.reportPage",
				"title": "TimeWalk: Show Today Report"
			},
			{
				"command": "timewalk.configFile",
				"title": "TimeWalk: Open Core Config File"
			},
			{
				"command": "timewalk.logFile",
				"title": "TimeWalk: Open Log File"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"dependencies": {
		"request": "^2.88.2",
		"adm-zip": "^0.4.14",
		"fs-extra": "^8.1.0",
		"typescript": "^3.8.3"
	},
	"devDependencies": {
		"@types/adm-zip": "^0.4.33",
		"@types/fs-extra": "^8.1.0",
		"@types/vscode": "^1.41.0",
		"@types/request": "^2.48.4",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.12.35",
		"glob": "^7.1.5",
		"mocha": "^6.2.3",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	}
}
